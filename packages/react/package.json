    "react-use": "^17.3.2",
    "timeago.js": "^4.0.2",
    "tiny-invariant": "^1.2.0",
    "tiny-warning": "^1.0.3",
    "tinycolor2": "^1.4.2",
    "tslib": "^2.3.1",
    "zustand": "^3.7.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  }
